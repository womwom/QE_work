&CONTROL
calculation = 'scf'
prefix = 'YBCO6'
restart_mode = 'from_scratch'
outdir='./YBCO6'
pseudo_dir='~/q-e/pseudo/all_pbe_UPF_v1.5'
verbosity     = 'high'
forc_conv_thr = 1D-3
etot_conv_thr = 1D-4
/
&SYSTEM
   ibrav            = 6 ! 6: tetragonal, 8: orthorombic
   a = 7.59153315636
   c = 11.5762603641
   ntyp             = 6
   nat              = 48
   ecutwfc = 60
   ecutrho = 540
   occupations = 'smearing'
   smearing = 'm-p'
   degauss = 0.01
   nspin = 2
   ! noncolin = .true.
   starting_magnetization(4) = 0.1
   starting_magnetization(5) = -0.1
/
&ELECTRONS
    conv_thr = 1d-8
    electron_maxstep = 500
    mixing_mode      = 'plain'
    mixing_beta      = 0.1
    ! diagonalization = 'cg'
    ! diago_david_ndim = 2
/
&IONS
ion_dynamics = 'bfgs'
/
&CELL
/

